{
  "name": "uhg_framework",
  "tree": {
    "$className": "DataModel",

    "ReplicatedStorage": {
      "$path": "src/replicatedstorage",
      "$ignoreUnknownInstances": true,
      "Events": {
        "$className": "Folder",
        "CharacterEvents": {
          "$className": "BindableEvent"
        }
      },
      "RemoteEvents": {
          "$className": "Folder",
          "ChangeCharacterSpeed": {
            "$className": "RemoteEvent"
          },
          "ListenForHumanoidState": {
            "$className": "RemoteEvent"
          },
          "ReturnAccurateHumanoidState": {
            "$className": "RemoteEvent"
          },
          "SendNotification": {
            "$className": "RemoteEvent"
          }
        }
    },

    "ServerScriptService": {
      "$path": "src/server",
      "PlayerSetup": {
        "$path": "src/playersetup"
      }
    },

    "ServerStorage": {
      "ServerDependencies": {
        "$path": "src/serverdependencies" 
      }
    },

    "StarterPlayer": {
      "StarterPlayerScripts": {
        "$path": "src/clientplr"
      },

      "StarterCharacterScripts": {
        "$path": "src/clientchar"
      }
    },

    "Workspace": {
      "$properties": {
        "FilteringEnabled": true
      }
    },
    "SoundService": {
      "$properties": {
        "RespectFilteringEnabled": true
      }
    }
  }
}